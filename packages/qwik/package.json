    "directory": "packages/qwik"
  },
  "bugs": {
    "url": "https://github.com/ZAHON/frame-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.qwik.cjs",
  "qwik": "./lib/index.qwik.mjs",
  "module": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib-types/index.d.ts",
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs"
    }
  },
  "files": [
    "./lib",
    "./lib-types"
  ],
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "scripts": {
    "build": "bun lint && bun clean && bun build:types && bun build:lib",
    "build:lib": "vite build --mode lib",
    "build:types": "tsc --emitDeclarationOnly --project ./tsconfig.build.json",
    "clean": "rm -rf ./lib ./lib-types ./tsconfig.build.tsbuildinfo",
    "dev": "vite --mode ssr",
    "fmt": "prettier --write . --ignore-path ../../.prettierignore",
    "fmt:check": "prettier --check . --ignore-path ../../.prettierignore",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "vitest --reporter verbose run",
    "test:watch": "vitest --reporter verbose",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.16.0",
